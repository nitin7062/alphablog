<h1> Articles</h1>
<table>
  <thead>
    <tr>
      <th> Id</th>
      <th> Title</th>
      <th> Description</th>
      <th> Actions</th>
      <th> Created_by_user</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each do|article|%>
    <tr>
      <td><%= article.id %> </td>
      <td><%= article.title %> </td>
      <td><%= article.description%> </td>
      <td><%= link_to 'delete', article, data:{turbo_method: :delete, turbo_confirm: "are you sure?"}%></td>
      <td><%= link_to 'edit' , edit_article_path(article) %></td>
      <td><%= link_to 'show' , article_path(article) %></td>
      <td><%= article.user.username%></td>
    </tr>
    <%end%>
  </tbody>
</table>